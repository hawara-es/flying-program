{
  "name": "flying-program",
  "version": "1.0.0",
  "description": "An framework-based attempt to write easy to read programs.",
  "main": "./src/index.js",
  "scripts": {
    "run": "node --experimental-modules ./src/program.mjs",
    "prepare-tools": "chmod +x ./tools/browserify.sh && chmod +x ./tools/samples/browserify.sh",
    "build": "./tools/browserify.sh",
    "build-samples": "./tools/samples/browserify.sh",
    "test": "mocha --recursive"
  },
  "keywords": [
    "program",
    "process",
    "function",
    "debug"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hawara-es/flying-program"
  },
  "dependencies": {
    "superstruct": "^0.6.0"
  },
  "devDependencies": {
    "mocha": "*"
  },
  "license": "GPL-3.0-or-later",
  "author": "Carlos Capote <carlos.capote@hawara.es>"
}
